#!/home/czepluch/Dropbox/prgrms/ethereum/raiden/.venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ipython','console_scripts','iptest2'
__requires__ = 'ipython'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ipython', 'console_scripts', 'iptest2')()
    )
